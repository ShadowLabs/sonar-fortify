<rules>
        <!-- see names and descriptions in org/sonar/l10n/ -->
    
          <rule>
            <key>obsolete</key><priority>MINOR</priority><configKey>Obsolete</configKey>
          </rule>
        
          <rule>
            <key>system_information_leak</key><priority>MINOR</priority><configKey>System Information Leak</configKey>
          </rule>
        
          <rule>
            <key>command_injection</key><priority>BLOCKER</priority><configKey>Command Injection</configKey>
          </rule>
        
          <rule>
            <key>cross_site_scripting_persistent</key><priority>BLOCKER</priority><configKey>Cross-Site Scripting: Persistent</configKey>
          </rule>
        
          <rule>
            <key>cross_site_scripting_poor_validation</key><priority>MINOR</priority><configKey>Cross-Site Scripting: Poor Validation</configKey>
          </rule>
        
          <rule>
            <key>cross_site_scripting_reflected</key><priority>BLOCKER</priority><configKey>Cross-Site Scripting: Reflected</configKey>
          </rule>
        
          <rule>
            <key>denial_of_service</key><priority>MINOR</priority><configKey>Denial of Service</configKey>
          </rule>
        
          <rule>
            <key>dynamic_code_evaluation_code_injection</key><priority>MAJOR</priority><configKey>Dynamic Code Evaluation: Code Injection</configKey>
          </rule>
        
          <rule>
