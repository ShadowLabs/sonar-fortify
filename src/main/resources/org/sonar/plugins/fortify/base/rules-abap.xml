          </rule>
        
          <rule>
            <key>password_management_hardcoded_password</key><priority>CRITICAL</priority><configKey>Password Management: Hardcoded Password</configKey>
          </rule>
        
          <rule>
            <key>password_management_password_in_comment</key><priority>MINOR</priority><configKey>Password Management: Password in Comment</configKey>
          </rule>
        
          <rule>
            <key>privacy_violation</key><priority>CRITICAL</priority><configKey>Privacy Violation</configKey>
          </rule>
        </rules>