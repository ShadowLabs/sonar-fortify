          </rule>
        
          <rule>
            <key>open_redirect</key><priority>CRITICAL</priority><configKey>Open Redirect</configKey>
          </rule>
        
          <rule>
            <key>path_manipulation</key><priority>MAJOR</priority><configKey>Path Manipulation</configKey>
          </rule>
        
          <rule>
            <key>process_control</key><priority>CRITICAL</priority><configKey>Process Control</configKey>
          </rule>
        
          <rule>
            <key>resource_injection</key><priority>MINOR</priority><configKey>Resource Injection</configKey>
          </rule>
        
          <rule>
            <key>sql_injection</key><priority>BLOCKER</priority><configKey>SQL Injection</configKey>
          </rule>
        
          <rule>
            <key>SF</key><priority>MAJOR</priority><configKey>Security Features</configKey>
          </rule>
        
          <rule>
            <key>access_control_database</key><priority>CRITICAL</priority><configKey>Access Control: Database</configKey>
          </rule>
        
          <rule>
